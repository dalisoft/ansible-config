[user]
  name = Davlatjon Shavkatov
  email = opensource@dalisoft.uz
[cola]
  spellcheck = false
[filter "lfs"]
  process = git-lfs filter-process
  required = true
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
[init]
  defaultBranch = master
[core]
  ignorecase = false
[advice]
  ignoredHook = false
[includeIf "gitdir:~/Desktop/Open_Source/"]
  path = oss.gitconfig
[includeIf "gitdir:~/Desktop/Workdir/"]
  path = commercial.gitconfig
