[user]
  name = Davlatjon Shavkatov
  email = opensource@dalisoft.uz
  signingKey = 3F3BD31DC9AE05DC22A8F4F520DBA1393DB06FC1
[cola]
  spellcheck = false
[filter "lfs"]
  process = git-lfs filter-process
  required = true
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
[init]
  defaultBranch = master
[alias]
  stat = status --show-stash -b -uall --renames
  re = reflog expire --expire=now --all
  ra = repack -ad
  p = prune
  f = fetch --all --prune
  si = submodule update --init --recursive --remote --rebase
  rb = !git f; git pull --rebase; gitsu
[core]
  ignorecase = false
[advice]
  ignoredHook = false
[commit]
  gpgsign = true
[tag]
  forceSignAnnotated = true
